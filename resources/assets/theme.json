{
  "theme": "light",
  "token": {
    "fontFamily": "'Inter', sans-serif",
    "colorPrimary": "#199A8E",    
    "wirefreame": false, 
    "colorLink": "#199A8E",
    "colorSuccess": "#34C759",     
    "colorWarning": "#FF9500",     
    "colorError": "#FE5C5C",       
    "colorText": "#222",        
    "colorBgBase": "#fff",  
    "borderRadius": 8,            
    "fontSize": 13,                      
    "paddingContentHorizontal": 16,
    "paddingContentVertical": 10,
    "motionDurationMid": "0.15s"
  },
  "components": {
    "Typography": {
      "fontSizeHeading1": 30,
      "fontSizeHeading2": 26,
      "fontSizeHeading3": 20,
      "fontSizeHeading4": 18,
      "fontSizeHeading5": 16,
      "marginXS": 0,
      "marginXXS": 0,
      "titleMarginBottom": 0,
      "titleMarginTop": 0
    },
    "Button": {
      "colorLink": "#174a47",
      "bgDanger": "#e11719",
      "controlOutline": 0
    },
    "Statistic": {
      "contentFontSize": 16
    },
     "Collapse": {
      "headerBg": "#fff"
    },
    "Avatar": {
      "colorTextPlaceholder": "#E8F3F1"
    }, 
    "Layout": {
      "bodyBg": "#fff",
      "siderBg": "#f9f9f9",
      "lightSiderBg": "#f9f9f9",
      "headerHeight": 50,
      "footerPadding": 2,
      "headerBg": "#fff",
      "headerPadding": "0 8px",
      "footerBg": "#fff"
    },
    "Card": {
      "colorBorderSecondary": "#d9d9d9",
      "padding": 12,
      "paddingLG": 12,
      "boxShadow": "none"
    },
    "Table": {
      "headerBg": "transparent",
      "padding": 6
    },
    "Divider": {
      "colorSplit": "rgb(0 0 0 / 15%)",
      "marginLG": 12
    },
    "Form": {
      "itemMarginBottom": 20,
      "labelColonMarginInlineEnd": 4
    },
    "Radio": {
      "controlHeight": 20
    },
    "Select": {
      "colorBgContainer": "#fff"
    },
    "Input": {
      "boxShadow": "none",
      "colorBgContainer": "#F9FAFB"
    }, 
    "DatePicker": {
      "colorBgContainer": "#F9FAFB"
    }, 
    "TimePicker": {
      "colorBgContainer": "#F9FAFB"
    },
    "Dropdown": {
      "dropdownWidth": 300,
      "padding": 0,
      "boxShadowSecondary": "0 0px 6px 1px rgba(0, 0, 0, 0.08)"
    },
    "Menu": {
      "lineWidth": 0,
      "lineHeight": 0,
      "margin": 2,
      "itemHeight": 30,
      "lineWidthFocus": 0,
      "dropdownWidth": 200,
      "activeBarBorderWidth": 0,
      "inlineIndent": 2,
      "itemPaddingInline": 2,
      "itemMarginInline": 2,
      "itemMarginBlock": 2,
      "iconMarginInlineEnd": 2,
      "iconSize": 18,
      "collapsedIconSize": 18
    }
  }
}